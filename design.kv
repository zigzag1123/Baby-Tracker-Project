<LoginWindow>:
    name: "login"

    FloatLayout:
        canvas.before:
            Color:
                rgba: (2/255,28/255,57/255,255/255)
            Rectangle:
                size: self.size
                pos: self.pos

        Image:
            source: "BTLogo.png"
            size: self.texture_size
            pos_hint: {'x': -0.15, 'y': .1}
            keep_ratio: False
        
        Label:
            text: "Username:"
            pos_hint: {'x': .65, 'y': .5}
            color: 2/255,28/255,57/255
            size_hint: .1, .1
            canvas.before:
                Color:
                    rgba: (65/255,180/255,247/255,255/255)
                Rectangle:
                    size: self.size
                    pos: self.pos

        TextInput:
            id: userw
            multiline: False
            write_tab: False
            pos_hint: {'x': .65, 'y': .45}
            size_hint: .1, .05
              
        Label:
            text: "Password:"
            pos_hint: {'x': .65, 'y': .3}
            color: 2/255,28/255,57/255
            size_hint: .1, .1
            canvas.before:
                Color:
                    rgba: (65/255,180/255,247/255,255/255)
                Rectangle:
                    size: self.size
                    pos: self.pos

        TextInput:
            id: passw
            password: True
            multiline: False
            write_tab: False
            pos_hint: {'x': .65, 'y': .25}
            size_hint: .1, .05

        Button:
            text: "Submit"
            pos_hint: {'x': .65, 'y': .15}
            color: 2/255,28/255,57/255
            size_hint: .1, .1
            background_normal: ''
            background_color: (65/255,180/255,247/255,255/255)
            on_release:
                app.loginProt()
              
        Button:
            text: "Create User"
            pos_hint: {'x': .1, 'y': .1}
            color: 2/255,28/255,57/255
            size_hint: .1, .1
            background_normal: ''
            background_color: (65/255,180/255,247/255,255/255)
            on_release:
                app.root.current = "creation"
                root.manager.transition.direction = "left"
              
